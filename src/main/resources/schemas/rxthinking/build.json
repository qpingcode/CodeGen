  "userParams": {
    "projectPrefix": "datatrans_"
  },
  "java":{
    "basePath": "/java",
    "basePackage": "com.rxthinking.datatrans",
    "ignoreTableName": ["t_", "tb_", "tbl_", "table_", "v_", "vw_", "view_", "dt_"],
    "ignoreColumnName": ["f_", "c_", "col_"],
    "list" : [
      {
        "fileTpl": "BeanPK",
        "fileName": "${table.alias}PK.java",
        "javaPackage": "bean",
        "condition": "@.jpa.PrimaryKeyCondition"
      },
      {
        "fileTpl": "Bean",
        "fileName": "${table.alias}.java",
        "javaPackage": "bean"
      },
      {
        "fileTpl": "Repository",
        "fileName": "${table.alias}Repository.java",
        "javaPackage": "repository"
      },
      {
        "fileTpl": "Service",
        "fileName": "${table.alias}Service.java",
        "javaPackage": "service"
      }
    ]
  }

}